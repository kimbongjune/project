<%@ page language="java" contentType="text/html; charset=utf-8"
    pageEncoding="utf-8"%>
<%@ include file="header.jsp" %>
<style>

element.style 
{
}
#content01 
{
    width: 100%;
    height: auto;
    margin: 0 auto;
    color: #777;
    border-top: 1px solid #e3e3e3;
}
* 
{
    margin: 0;
    padding: 0;
    border: 0;
    vertical-align: top;
}
div 
{
    display: block;
}
body {
    font-family: "Nanum Gothic", "Malgun Gothic", "돋움", Dotum,Arial,sans-serif;
    font-size: 14px;
    line-height: 1.3em;
}
.cultureSearch {
    width: 97%;
    height: 86px;
    background-color: #1a4653;
    margin: 0 auto;
    z-index: 0;
    margin-top: 5px;
    padding: 0px;
}
.cultureSearch .cultureBox {
    float: left;
    width: 86%;
    height: 60px;
    float: left;
    font-size: 15px;
    text-indent: 25px;
    color: #aaa;
    margin: 11px 0 30px 13px;
    border: 1px solid #777;
    line-height: 60px;
}
.cultureSearch .cultureBtn {
    float: right;
    width: 6%;
    height: 60px;
    background-position: center;
    background-color: #1a4653;
    border: 1px solid #fff;
    margin: 12px 15px 0 0px;
    cursor: pointer;
    font-size: 17px;
    color: #fff;
    margin-right: 15px;
}
p {
    display: block;
    margin-block-start: 1em;
    margin-block-end: 1em;
    margin-inline-start: 0px;
    margin-inline-end: 0px;
}
#main_bg {
    text-align: center;
    width: 100%;
    height: auto;
    min-height: 120px;
    margin-bottom: 20px;
    padding-top: 13px;
    padding-bottom: 13px;
    background-position: center;
    font-family: "Nanum Gothic", "Malgun Gothic", "돋움", Dotum,Arial,sans-serif;
    font-size: 14px;
    line-height: 1.7em;
    display: inline-block;
}
.mainWrap {
    width: 1100px;
    height: auto;
    margin: 0 auto;
    padding-bottom: 35px;
    padding-top: 10px;
}
.info_main_bg {
    background-color: #d45a77;
    padding-bottom: 30px;
}
.red {
    color: #ff0000;
}
.month_best h3 {
    text-align: center;
    width: 100%;
    font-size: 150%;
    margin-top: 10px;
    font-weight: normal;
}
h3 {
    padding: 0;
    font-size: 20px;
    color: #111;
    line-height: 1.3em;
}
.month_best {
    width: 100%;
    height: auto;
    margin: 0 auto;
    float: left;
    margin-bottom: 30px;
}
.month_best h4 {
    text-align: center;
    width: 100%;
    font-size: 100%;
    margin-top: 10px;
    font-weight: normal;
}
.month_best .month_list {
    float: left;
    margin-top: 20px;
    border-top: 1px solid #454545;
    width: 100%;
}
.line {
    float: left;
    border-top: 1px solid #777;
    height: 5px;
    width: 99%;
}
.serchResult {
    display: inline-block;
    width: 100%;
    height: auto;
    padding-top: 10px;
    padding-bottom: 10px;
    margin-top: 0px;
    border-bottom: 1px solid #454545;
}
img, video {
    max-width: 100%;
    border: none;
}
.month_best .month_list .monthWrap img 
{
	max-width: 1120px;
    border: 1px solid #ddd;
    width: 330px;
    height: 220px;
}
.stay_cate {
    color: #e01348;
    margin-right: 7px;
}
div.con_area {
	max-width: 1119px;
    width: 98%;
    margin: 0 auto;
    padding: 0;
}
.con_area 
{
	min-width: 1px;
	max-width: 979px;
    width: 100%;
    margin: 0 auto;
    padding: 0;
}
.month_best .month_list .monthWrap {
    position: relative;
}

    .top_wrap .font-size {
        float: right;
        margin-right: 15px;
        white-space: nowrap;
        margin-top: 10px;
    }

    .top_wrap .font-size span {
        line-height: 16px;
        font-size: 12px;
        color: #666;
    }

    .top_wrap .font-size button {
        background-color: #555;
        font-size: 12px;
        border: 0;
        color: #aaa;
        height: 18px;
        padding: 0 5px;
        line-height: 18px;
        letter-spacing: -0.5px;
    }
    
	h2{
		padding-top : 10px;
	}
	.month_best .month_list .monthWrap { position:relative;}
	.photoWrap { position:relative;}
	.disable { position:absolute; top:0; right:10px; width:65px; height:67px; z-index:10;}
	.disable img {  height:auto !important; border:none !important; }
	.month_best .month_list .monthWrap {
    display: inline-block;
    width: 30%;
    height: auto;
    margin: 30px 1% 0 2%;
    float: left;
}
.serchResult .result li {
    float: left;
    margin-right: 10px;
    font-size: 15px;
}
li {
    display: list-item;
    text-align: -webkit-match-parent;
}
ul, ol, dl {
    list-style: none;
}
ul {
    list-style-type: disc;
}
.order li .on {
    background-color: #4c4c4c;
    color: #fff;
}
.order li a {
    float: left;
    width: 70px;
    height: 25px;
    padding: 3px;
    border: 1px solid #909090;
    text-align: center;
    line-height: 23px;
    background-color: #fcfcfc;
    font-size: 13px;
}
.order li {
    float: left;
    margin-left: 5px;
    font-size: 14px;
}
a:-webkit-any-link {
    color: -webkit-link;
    cursor: pointer;
}
.order {
    float: right;
    margin-top: 10px;
    margin-right: 10px;
}
a {
    text-decoration: none;
    color: #777;
}
a:link, a:visited {
    color: #555;
}
.serchResult .result .result_on {
    font-weight: bold;
    color: #da2737;
    font-size: 16px;
}
.serchList {
    display: inline-block;
    width: 100%;
    height: auto;
    margin: 0 auto;
    padding-bottom: 30px;
    border-bottom: 0px solid #454545;
}
.photoWrap .jb_area {
    display: block;
    width: auto;
    height: auto;
    padding: 0% 2% 0% 1%;
    background-color: #d92737;
    color: #fff;
    font-size: 85%;
    text-align: center;
    float: left;
    margin-right: 10px;
    border-radius: 8px;
}
.photoWrap {
    display: inline-block;
    width: 32%;
    max-height: 430px;
    min-height: 350px;
    margin: 20px 1px 0 10px;
    overflow: hidden;
    cursor: pointer;
    float: left;
}
.stay_infobox li {
    width: 98%;
    padding: 1%;
    font-size: 90%;
    padding-top: 10px;
}
.photoWrap a {
    display: block;
    width: 100%;
    font-size: 110%;
    color: #2d2d2d;
    line-height: 33px;
    text-indent: 5px;
    font-weight: 500;
}
.photoWrap .photo_img {
    display: inline-block;
    width: 97%;
    height: 240px;
    text-align: center;
    background: #f1f1f1;
    margin-bottom: 10px;
    border: 1px solid #ddd;
}
.stay_infobox {
    float: left;
    width: 95%;
    margin-top: 5px;
    margin-bottom: 15px;
    border-top: 1px solid #d4d4d4;
    padding: 1%;
    margin-left: 2%;
    font-size: 100%;
    line-height: 1.5em;
    height: 120px;
    overflow: hidden;
}
#count {
    display: inline-block;
    width: 100%;
    margin-top: 10px;
    border-top: 1px solid #777;
}
#count > ul>li {
    display: block;
    width: 35px;
    height: 35px;
    font-size: 13px;
    text-align: center;
    line-height: 35px;
    color: rgb(153, 153, 153);
    border: 1px solid rgb(221, 221, 221);
    cursor: pointer;
}
#count .page strong {
    float: left;
    height: 35px;
    margin: 0;
    padding: 0px 13px;
    color: #fff;
    font-weight: bold;
    line-height: 35px;
    border: 1px solid #ddd;
    background-color: #eb060e;
}
#count ul {
    display: table;
    width: auto;
    margin: 25px auto;
    padding-top: 10px;
}
#count li {
    float: left;
    margin: 0 2px;
}
#count li > a {
    display: block;
    width: 35px;
    height: 35px;
    font-size: 13px;
    text-align: center;
    line-height: 35px;
    color: #999;
    border: 1px solid #ddd;
    cursor: pointer;
}
#count .page a:hover, #content01 #count .page a:focus {
    color: #fff;
    background-color: #eb060e;
}
.photoWrap:hover 
{
	opacity: 0.7;
}
.monthWrap:hover 
{
	opacity: 0.7;
}


</style>
	<!-- 리스트페이지 전체를 감싸는 div -->
	<div id="#content01">
		<div id="main_bg" class="info_main_bg">
			<!-- 검색 -->
			<div class="mainWrap">
				<form name="travelSearchForm" id="travelSearchForm" action="#" method="post">
					<div class="cultureSearch">
						<input type="text" class="cultureBox" name="sk" value="" placeholder="찾으시는 숙박업소가 있습니까?">
						<input type="submit" class="cultureBtn" name="cultureBtn" value="검색">
					</div>
				</form>
			</div>
			<!-- 검색 끝 -->
		</div>
		<!-- 리스트페이지 감싸는 글나오는곳을 감싸는 div -->
		<div class="con_area"> 
			<!-- 탑3 를 감싸는 div -->
			<div class="month_best">
				
				<h3>이달의 <span class="red">추천</span> 숙박업소</h3>
				<h4>전북의 숙박을 매달 추천해드립니다. 잠자리가 편하면 여행이 더 즐거워집니다!</h4>
			
				<!-- 탑3 -->
				<div class="month_list">
					<% 
					for(int i = 0 ; i < 3 ; i++)
					{
					%>
					<div class="monthWrap">
						<a href="#"><!-- 링크 넣을곳  -->
						<img src="../image/d.png" alt="20090812_1049_381.JPG" class="photo_img"><!-- 이미지 넣을곳  -->
						</a>
						<p>[무주]<br><!-- 동넣을곳 --><span class="stay_cate">숙박</span><!-- 숙박인지 먹는곳인지 -->
							<a href="#">무주눈꽃여행</a><!-- 제목 -->
							<br>
							☆☆☆☆☆  후기 0개<!-- 좋아요 넣으면 될듯  -->
						</p>
					</div>		
					<%
					}
					%>									
				</div>
				<!-- 탑3끝 -->
			</div>
			<!-- 탑3 감싸는 div끝 -->
			<p class="line"></p><!-- 선긋는 p -->
			
			<!-- 검색값뿌려주는 div -->
			<div class="serchResult" id="serchResult">
				<ul class="result">
					<li>검색결과</li>
					<li>|</li>
					<li>총<span class="result_on">1143</span>건</li>
					<li class="result_my">					
					</li>
				</ul>
			<ul class="order">
				<li><a href="#none" onclick="$('input[name=order_by]').val(1);$('#travelSearchForm').submit();" class="on">인기순</a></li>
				<li><a href="#none" onclick="$('input[name=order_by]').val(2);$('#travelSearchForm').submit();">가나다순</a></li>
			</ul>
			</div>
			<!-- 검색값뿌려주는 div 끝 -->
			
			<!-- 글리스트 보여주는곳  -->
			<div class="serchList">
				<% 
				for(int i = 0 ; i < 9 ; i++)
				{
				%>
				<div class="photoWrap">
					<a href="#">
						<img src="../image/d.png" alt="02.jpg" class="photo_img"><!-- 이미지 넣을곳 -->
					</a>
					<h3>
						<a href="#">
							<span class="jb_area">남원</span>	<!-- 위치 -->
							홀인원 무인텔	<!-- 제목 -->
						</a>
					</h3>
					<ul class="stay_infobox">
						<li>전북 남원시 주천면 장백산로 156 </li><!-- 주소 -->
					</ul>
				</div>
				<%
				}
				%>
			</div>
			<!-- 글리스트 보여주는곳 끝 -->
			
			<!-- 페이징  -->
			<div id="count">
				<ul>
					<li><a onclick="fnCtntSearchForm(1); ">&lt;&lt;</a></li><!-- 첫페이지가기 -->
					<li><a onclick="fnCtntSearchForm(1); ">&lt;</a></li><!-- 앞페이지가기 -->
					<%
					for(int i = 1 ; i <= 5 ; i++)
					{
						//현재 페이지 빨간색으로 강조할부분
						if(i == 1)
						{
							%>
								<li class="page"><strong><%= i %></strong></li>
							<%
							continue;
						}
					%>
							<li class="page"><a onclick="fnCtntSearchForm(<%= i %>); "><%= i %></a></li>
					<%
					}
					%>
					<li><a onclick="fnCtntSearchForm(6); ">&gt;</a></li><!-- 다음 페이지가기 -->
					<li><a onclick="fnCtntSearchForm(127); ">&gt;&gt;</a></li><!-- 마지막 페이지가기 -->
				</ul>
			</div>
			<!-- 페이징 끝 -->
		</div>
		<!-- 글리스트 감싸는 div 끝  -->
	</div>
	<!-- 전체 div 끝  -->
<%@ include file="footer.jsp" %>