/* Admin css 시작 */



.vertical1
{
	width:100%;
	text-align:right;
	font-size:30px;
   	color:black;
   	
}
#count {
    display: inline-block;
    width: 100%;
    margin-top: 10px;
    /* border-top: 1px solid #777; */
}
#count > ul>li {
    display: block;
    width: 35px;
    height: 35px;
    font-size: 13px;
    text-align: center;
    line-height: 35px;
    color: rgb(153, 153, 153);
    border: 1px solid rgb(221, 221, 221);
    cursor: pointer;
}
#count .page strong {
    float: left;
    height: 35px;
    margin: 0;
    padding: 0px 13px;
    color: #fff;
    font-weight: bold;
    line-height: 35px;
    border: 1px solid #ddd;
    background-color: #eb060e;
}
#count ul {
    display: table;
    width: auto;
    margin: 25px auto;
    padding-top: 10px;
}
#count li {
    float: left;
    margin: 0 2px;
}
#count li > a {
    display: block;
    width: 35px;
    height: 35px;
    font-size: 13px;
    text-align: center;
    line-height: 35px;
    color: #999;
    border: 1px solid #ddd;
    cursor: pointer;
}
#count .page a:hover, #content01 #count .page a:focus {
    color: #fff;
    background-color: #eb060e;
}
