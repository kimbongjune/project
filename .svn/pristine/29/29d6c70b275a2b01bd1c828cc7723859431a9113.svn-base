/* notice_list 시작 */
.notice_listdiv
{
	width:52%;
	margin:0 auto;
	text-align:center;
	position: relative;
}
.notice_listTable
{
	width:100%; 
	height:100%; 
	text-align:center;
	border-collapse:collapse;
	border-color:grey;
	position:relative;
}
.notice_ListNo
{
	width:80px;
}
.notice_ListHit
{
	width:80px;
}

.notice_ListWriter
{
	width:180px;
	
}
.notice_ListDate
{
	width:120px;
}

.notice_listContent
{
	border-top: 1px solid #E6E6E6;
	border-bottom: 1px solid #E6E6E6;	
}

.notice_listContent:hover
{
	background-color:#F9F9F9;
	cursor: pointer;
	
}

.notice_ListSearch
{
	height:100%;
	width:52%;
	position:relative;
    margin:0 auto;
	text-align:center;
	margin-left:44%;
}

.notice_ListSearch a
{
	width: 35px;
    height: 35px;
    font-size: 13px;
    text-color:black;
    text-align: center;
    line-height: 35px;
    color: #999;
    border: 1px solid #ddd;
    cursor: pointer;
    float: left;
    margin: 0 2px;
}

.notice_ListSearch a:hover
{
	background-color:red;
}

.notice_ListSearch a:visited
{
	color:black;
	text-color:black;
	text-decoration:none;
}
	
.notice_ListTh
{
	background-color: #F9F9F9;
	border: none;
	border-top: solid black 1px;
	height:45px;
}
/* notice_list 끝 */







