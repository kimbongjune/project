package tv.vo;

import java.util.ArrayList;

public class FestivalListVo 
{
	private ArrayList<FestivalVo> list;
	
	//생성자
		public FestivalListVo()	{	list = new ArrayList<FestivalVo>();	}
		
		//목록 1개를 추가한다.
		public void Add(FestivalVo item){	list.add(item);	}	
		
		//목록 1개를 얻는다.
		public FestivalVo Get(int index){	return list.get(index);	}
		
		//가지고 있는 전체 갯수 리턴
		public int Total(){	return list.size();	}
		
		public static void main(String[] args){}
	
}
